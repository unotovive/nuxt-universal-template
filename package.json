{
  "name": "LAB",
  "version": "1.0.0",
  "description": "LAB",
  "author": "LAB",
  "private": true,
  "scripts": {
    "dev": "cross-env DOT_ENV=development nuxt & yarn serve-map",
    "build": "cross-env DOT_ENV=production nuxt build",
    "start": "cross-env DOT_ENV=production nuxt start",
    "generate": "cross-env DOT_ENV=production nuxt generate",
    "serve-map": "browser-sync start --config 'bs-config.js'",
    "fixes": "run-s -l *:fix",
    "eslint:fix": "eslint --fix \"**/*.{js,vue}\" --ignore-path .gitignore",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue}\" --fix --ignore-path .gitignore",
    "prettier:fix": "prettier --write \"**/*.{css,scss,js,vue}\" --loglevel error --ignore-path .gitignore",
    "checks": "node ./lib/coding-ruler",
    "checks-all": "node ./lib/coding-ruler --all",
    "maps": "node ./lib/maps",
    "storybook": "start-storybook -p 20172",
    "build-storybook": "build-storybook",
    "test": "jest --detectOpenHandles",
    "test-serial": "jest -i",
    "test-console": "jest --silent=false --verbose false --onlyChanged",
    "test-console-watch": "jest --silent=false --verbose false --watch",
    "coverage": "jest --coverage --maxWorkers=2"
  },
  "lint-staged": {
    "*.{vue,svg,css,scss,js}": [
      "yarn checks"
    ]
  },
  "dependencies": {
    "@nuxt/babel-preset-app": "2.6.3",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "1.3.0",
    "@nuxtjs/pwa": "3.0.0-beta.16",
    "@nuxtjs/style-resources": "0.1.2",
    "await-timeout": "0.4.0",
    "change-case": "3.1.0",
    "intersection-observer": "0.7.0",
    "is-array": "1.0.1",
    "is-regexp": "2.1.0",
    "lodash.clonedeep": "4.5.0",
    "nuxt": "^2.4.0",
    "nuxt-imagemin": "1.0.0",
    "nuxt-polyfill": "1.0.3",
    "nuxt-rfg-icon": "0.6.3",
    "nuxt-user-agent": "1.2.1",
    "sanitize.css": "8.0.0",
    "smoothscroll-polyfill": "0.4.4",
    "url-polyfill": "1.1.5",
    "vue-spinner": "1.0.3",
    "vuex-map-fields": "1.3.4"
  },
  "devDependencies": {
    "@babel/preset-env": "7.4.4",
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "@storybook/addon-centered": "5.0.11",
    "@storybook/addon-knobs": "5.0.11",
    "@storybook/addon-links": "5.0.11",
    "@storybook/addon-notes": "5.0.11",
    "@storybook/addon-viewport": "5.0.11",
    "@storybook/vue": "5.0.11",
    "@vue/test-utils": "^1.0.0-beta.27",
    "autoprefixer": "9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.6",
    "babel-preset-vue": "2.0.2",
    "browser-sync": "2.26.5",
    "cross-env": "5.2.0",
    "css-mqpacker": "7.0.0",
    "eslint": "5.16.0",
    "eslint-config-google": "0.13.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-vue": "5.2.2",
    "husky": "2.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^23.6.0",
    "jest-canvas-mock": "2.1.0",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "8.1.6",
    "node-sass": "4.12.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "4.1.5",
    "postcss-gap-properties": "2.0.0",
    "prettier": "1.17.1",
    "sass-loader": "7.1.0",
    "sass-resources-loader": "2.0.0",
    "storybook-addon-vue-info": "1.2.0",
    "stylelint": "10.0.1",
    "stylelint-config-property-sort-order-smacss": "4.0.2",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.6.1",
    "stylelint-webpack-plugin": "0.10.5",
    "vue-jest": "^3.0.2",
    "vue-svg-loader": "0.12.0"
  }
}
